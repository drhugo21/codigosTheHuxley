tipoTrabalhador = str(input())

if(tipoTrabalhador == "PV"):
    sexo = str(input())
    idade = int(input())
    tempoC = int(input())
    if(sexo == "M" or sexo == "m"):
        if(idade >= 65 and tempoC >= 20):
            print("Trabalhador Privado pode se aposentar.")
        else:
            print("Trabalhador Privado NAO pode se aposentar!")
    elif(sexo == "F" or sexo == "f"):
        if(idade >= 62 and tempoC >= 20):
            print("Trabalhador Privado pode se aposentar.")
        else:
            print("Trabalhador Privado NAO pode se aposentar!")

elif(tipoTrabalhador == "TR"):
    sexo = str(input())
    idade = int(input())
    tempoC = int(input())
    if(idade >= 60 and tempoC >= 20):
        print("Trabalhador Rural pode se aposentar.")
    else:
        print("Trabalhador Rural NAO pode se aposentar!")

elif(tipoTrabalhador == "SP"):
    sexo = str(input())
    idade = int(input())
    tempoC = int(input())
    tempoSP = int(input())
    tempoCargo = int(input())
    if(sexo == "M" or sexo == "m"):
        if(idade >= 65 and tempoC >= 25 and tempoSP >= 10 and tempoCargo >= 5):
            print("Servidor publico pode se aposentar.")
        else:
            print("Servidor publico NAO pode se aposentar.")
    elif(sexo == "F" or sexo == "f"):
        if(idade >= 62 and tempoC >= 20 and tempoSP >= 10 and tempoCargo >= 5):
            print("Servidora publica pode se aposentar.")
        else:
            print("Servidor publico NAO pode se aposentar.")

elif(tipoTrabalhador == "PR"):
    sexo = str(input())
    idade = int(input())
    tempoC = int(input())
    if(idade >= 60 and tempoC >= 30):
        print("Professor pode se aposentar.")
    else:
        print("Professor NAO pode se aposentar!")

elif(tipoTrabalhador == "PC"):
    sexo = str(input())
    idade = int(input())
    tempoC = int(input())
    if(sexo == "M" or sexo == "m"):
        if(idade >= 55 and tempoC >= 30):
            tempoExercicio = int(input())
            if(tempoExercicio == 20):
                print("Policial pode se aposentar.")
            else:
                print("Policial NAO pode se aposentar.")
        else:
            print("Policial NAO pode se aposentar!")
    elif(sexo == "F" or sexo == "f"):
        if(idade >= 55 and tempoC >= 25):
            tempoExercicio = int(input())
            if(tempoExercicio == 15):
                print("Policial pode se aposentar.")
            else:
                print("Policial NAO pode se aposentar.")
        else:
            print("Policial NAO pode se aposentar!")
else:
    print("Categoria de trabalhador invalida.")
    
