            precoF = precoT - (precoT * 0.02)
            print("R$:","{:.2f}".format(precoF))
    elif(bebidaA == "S"):
        precoCerveja = (quantA*2) * 8
        precoT = precoCarneBoi + precoCarneFrango + precoCarneBoiC + precoCerveja
        if(paoAlho == "S"):
            print("R$:","{:.2f}".format(precoT))
        elif(paoAlho == "N"):
            precoF = precoT - (precoT * 0.02)
            print("R$:","{:.2f}".format(precoF))
            print("R$:","{:.2f}".format(precoF))
    elif(bebidaC == "S"):
        precoRefri = (quantC*0.5) * 6
        precoT = precoCarneBoi + precoCarneFrango + precoCarneBoiC + precoRefri
        if(paoAlho == "S"):
            print("R$:","{:.2f}".format(precoT))
        elif(paoAlho == "N"):
            precoF = precoT - (precoT * 0.02)
            print("R$:","{:.2f}".format(precoF))
            print("R$:","{:.2f}".format(precoF))
    else:
        if(paoAlho == "S"):
            print("R$:","{:.2f}".format(precoCarnes))
        elif(paoAlho == "N"):
            precoF = precoCarnes - (precoCarnes * 0.02)
            print("R$:","{:.2f}".format(precoF))
            print("R$:","{:.2f}".format(precoF))

